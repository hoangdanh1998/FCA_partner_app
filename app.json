{
  "expo": {
    "name": "FCA_Partner",
    "slug": "FCA_Partner",
    "version": "1.0.0",
    "orientation": "landscape",
  
    "icon": "./assets/fast-coffee-img-logo.png",
    "splash": {
      "image": "./assets/fast-coffee-img-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#e6d7ab"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/fast-coffee-img-logo.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "pan"
    },
    "notification": {
      "icon": "./assets/fast-coffee-img-logo.png"
    },
    "web": {
      "favicon": "./assets/fast-coffee-img-logo.png"
    }
  }
}
